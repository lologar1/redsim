{
    "credit": "Made by Nekzuris",
    "multipart": [
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply": { "model": "block/glass/glass_pane_post_0" }
        },
        {   "when": { "north": true, "east": false, "south": false, "west": false },
            "apply": { "model": "block/glass/glass_pane_post_1" }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": false },
            "apply": { "model": "block/glass/glass_pane_post_1", "y": 90 }
        },
        {   "when": { "north": false, "east": false, "south": true, "west": false },
            "apply": { "model": "block/glass/glass_pane_post_1", "y": 180 }
        },
        {   "when": { "north": false, "east": false, "south": false, "west": true },
            "apply": { "model": "block/glass/glass_pane_post_1", "y": 270 }
        },
        {   "when": { "north": true, "east": false, "south": true, "west": false },
            "apply": { "model": "block/glass/glass_pane_post_2" }
        },
        {   "when": { "north": false, "east": true, "south": false, "west": true },
            "apply": { "model": "block/glass/glass_pane_post_2", "y": 90 }
        },
        {   "when": { "north": true, "east": true, "south": false, "west": false },
            "apply": { "model": "block/glass/glass_pane_post_2a" }
        },
        {   "when": { "north": false, "east": true, "south": true, "west": false },
            "apply": { "model": "block/glass/glass_pane_post_2a", "y": 90 }
        },
        {   "when": { "north": false, "east": false, "south": true, "west": true },
            "apply": { "model": "block/glass/glass_pane_post_2a", "y": 180 }
        },
        {   "when": { "north": true, "east": false, "south": false, "west": true },
            "apply": { "model": "block/glass/glass_pane_post_2a", "y": 270 }
        },
        {   "when": { "north": false, "east": true, "south": true, "west": true },
            "apply": { "model": "block/glass/glass_pane_post_3" }
        },
        {   "when": { "north": true, "east": false, "south": true, "west": true },
            "apply": { "model": "block/glass/glass_pane_post_3", "y": 90}
        },
        {   "when": { "north": true, "east": true, "south": false, "west": true },
            "apply": { "model": "block/glass/glass_pane_post_3", "y": 180}
        },
        {   "when": { "north": true, "east": true, "south": true, "west": false },
            "apply": { "model": "block/glass/glass_pane_post_3", "y": 270}
        },
        {   "when": { "north": true, "east": true, "south": true, "west": true },
            "apply": { "model": "block/glass/glass_pane_post_4"}
        },
        
        {   "when": { "north": true },
            "apply": { "model": "block/glass/glass_pane_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "block/glass/glass_pane_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "block/glass/glass_pane_side", "y": 180 }
        },
        {   "when": { "west": true },
            "apply": { "model": "block/glass/glass_pane_side", "y": 270 }
        }
    ]
}
