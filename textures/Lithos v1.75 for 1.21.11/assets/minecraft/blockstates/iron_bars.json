{
    "multipart": [
        {   "apply": 
        	{ "model": "block/built/iron_bars_post_ends" }
        },
        {   "when": { "north": false, "east": false, "south": false, "west": false },
            "apply":
            { "model": "block/built/iron_bars_post" }
        },
        {   "when": { "north": true },
            "apply": 
            { "model": "block/built/iron_bars_side" }
        },
        {   "when": { "east": true },
            "apply": 
            { "model": "block/built/iron_bars_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": 
            { "model": "block/built/iron_bars_side_alt" }
        },
        {   "when": { "west": true },
            "apply": 
            { "model": "block/built/iron_bars_side_alt", "y": 90 }
        }
    ]
}
