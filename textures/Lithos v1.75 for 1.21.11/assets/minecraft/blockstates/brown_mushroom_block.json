{
    "multipart": [
        {   "when": { "north": true },
            "apply": [
            	{ "model": "block/mushrooms/brown_block_side_1" },
            	{ "model": "block/mushrooms/brown_block_side_2" },
            	{ "model": "block/mushrooms/brown_block_side_3" }
            	]
        },
        {   "when": { "east": true },
            "apply": [
            	{ "model": "block/mushrooms/brown_block_side_1", "y": 90, "uvlock": true },
            	{ "model": "block/mushrooms/brown_block_side_2", "y": 90, "uvlock": true },
            	{ "model": "block/mushrooms/brown_block_side_3", "y": 90, "uvlock": true }
            	]
        },
        {   "when": { "south": true },
            "apply": [
            	{ "model": "block/mushrooms/brown_block_side_1", "y": 180, "uvlock": true },
            	{ "model": "block/mushrooms/brown_block_side_2", "y": 180, "uvlock": true },
            	{ "model": "block/mushrooms/brown_block_side_3", "y": 180, "uvlock": true }
            	]
        },
        {   "when": { "west": true },
            "apply": [
            	{ "model": "block/mushrooms/brown_block_side_1", "y": 270, "uvlock": true },
            	{ "model": "block/mushrooms/brown_block_side_2", "y": 270, "uvlock": true },
            	{ "model": "block/mushrooms/brown_block_side_3", "y": 270, "uvlock": true }
            	]
        },
        {   "when": { "up": true },
            "apply": [
            	{ "model": "block/mushrooms/brown_block_1", "x": 270, "uvlock": true },
            	{ "model": "block/mushrooms/brown_block_2", "x": 270, "uvlock": true },
            	{ "model": "block/mushrooms/brown_block_3", "x": 270, "uvlock": true }
            	]
        },
        {   "when": { "down": true },
            "apply": [
            	{ "model": "block/mushrooms/brown_block_1", "x": 90, "uvlock": true },
            	{ "model": "block/mushrooms/brown_block_2", "x": 90, "uvlock": true },
            	{ "model": "block/mushrooms/brown_block_3", "x": 90, "uvlock": true }
            	]
        },
        {   "when": { "north": false },
            "apply": { "model": "block/mushrooms/brown_block_inside" }
        },
        {   "when": { "east": false },
            "apply": { "model": "block/mushrooms/brown_block_inside", "y": 90, "uvlock": false }
        },
        {   "when": { "south": false },
            "apply": { "model": "block/mushrooms/brown_block_inside", "y": 180, "uvlock": false }
        },
        {   "when": { "west": false },
            "apply": { "model": "block/mushrooms/brown_block_inside", "y": 270, "uvlock": false }
        },
        {   "when": { "up": false },
            "apply": { "model": "block/mushrooms/brown_block_inside", "x": 270, "uvlock": false }
        },
        {   "when": { "down": false },
            "apply": { "model": "block/mushrooms/brown_block_inside", "x": 90, "uvlock": false }
        }
    ]
}
